    @property
    def PostShoulderLength(self):
        return self.__WallThick+2.52
    @property
    def PostLength(self):
        return self.CaseBottomHeight-self.__BoardThick
    @property
    def TopPostLength(self): 
        return self.CaseTopHeight-self.__WallThick
    @property
    def TopPostDrillDepth(self):
        return self.__SpaceAboveBoard
    @property
    def CaseWidth(self):
        w,l = self.__BoardSize
        return w+(self.__WallThick*2)
    @property
    def CaseLength(self):
        w,l = self.__BoardSize
        return l+(self.__WallThick*2)
    @property
    def MountTabCY1(self):
        return self.CaseLength*.25
    @property
    def MountTabCY2(self):
        return self.CaseLength*.75
    @property
    def CaseBottomHeight(self):
        return self.__WallThick+self.__SpaceUnderBoard+self.__BoardThick
    @classmethod
    def ZOffset(cls):
        return (cls.__WallThick+cls.__SpaceUnderBoard+cls.__BoardThick)-2.33
    @property
    def CaseTopHeight(self):
        return self.__WallThick+self.__SpaceAboveBoard+(self.__WallThick/2)
    def __bottom(self):
        bot = Part.makePlane(self.CaseWidth,self.CaseLength,self.origin) \
            .extrude(Base.Vector(0,0,self.__WallThick))
        bot = bot.fuse(
            Part.makePlane(self.CaseBottomHeight,self.CaseLength,
                           self.origin.add(Base.Vector(0,self.CaseLength,0)),
                           Base.Vector(1,0,0)).extrude(Base.Vector(self.__WallThick,0,0))
        )
        bot = bot.fuse(
            Part.makePlane(self.CaseBottomHeight,self.CaseLength,
                           self.origin.add(Base.Vector(self.CaseWidth-self.__WallThick,
                                                       self.CaseLength,0)),
                           Base.Vector(1,0,0)).extrude(Base.Vector(self.__WallThick,0,0))
        )
        bot = bot.fuse(
            Part.makePlane(self.CaseBottomHeight,self.CaseWidth,
                           self.origin,
                           Base.Vector(0,1,0)).extrude(Base.Vector(0,self.__WallThick,0))
        )
        bot = bot.fuse(
            Part.makePlane(self.CaseBottomHeight,self.CaseWidth,
                           self.origin.add(Base.Vector(0,self.CaseLength-self.__WallThick,0)),
                           Base.Vector(0,1,0)).extrude(Base.Vector(0,self.__WallThick,0))
        )
        temp = Part.makePlane(self.__WallThick,self.__WallThick,self.origin,
                              Base.Vector(0,1,0))\
                .extrude(Base.Vector(0,self.CaseLength,0))
        offset = Base.Vector(self.__WallThick,0,self.__WallThick)
        rod = Part.makeCylinder(self.__WallThick,self.CaseLength,
                                self.origin.add(offset),Base.Vector(0,1,0))
        bot = bot.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              self.origin.add(Base.Vector(self.CaseWidth-self.__WallThick,0,0)),
                              Base.Vector(0,1,0))\
                .extrude(Base.Vector(0,self.CaseLength,0))
        offset = Base.Vector(self.CaseWidth-self.__WallThick,0,self.__WallThick)
        rod = Part.makeCylinder(self.__WallThick,self.CaseLength,
                                self.origin.add(offset),Base.Vector(0,1,0))
        bot = bot.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              self.origin.add(Base.Vector(0,self.__WallThick,0)),
                              Base.Vector(1,0,0)).extrude(Base.Vector(self.CaseWidth,0,0))
        offset = Base.Vector(0,self.__WallThick,self.__WallThick)
        rod = Part.makeCylinder(self.__WallThick,self.CaseWidth,
                                self.origin.add(offset),Base.Vector(1,0,0))
        bot = bot.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              self.origin.add(Base.Vector(0,self.CaseLength,0)),
                              Base.Vector(1,0,0)).extrude(Base.Vector(self.CaseWidth,0,0))
        offset = Base.Vector(0,self.CaseLength-self.__WallThick,self.__WallThick)
        rod = Part.makeCylinder(self.__WallThick,self.CaseWidth,
                                self.origin.add(offset),Base.Vector(1,0,0))
        bot = bot.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              self.origin).extrude(Base.Vector(0,0,self.CaseBottomHeight))
        rod = Part.makeCylinder(self.__WallThick,self.CaseBottomHeight,
                                self.origin.add(Base.Vector(self.__WallThick,self.__WallThick,0)))
        #self.t = temp
        #self.r = rod
        bot = bot.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              self.origin.add(Base.Vector(0,self.CaseLength-self.__WallThick,0))
                              ).extrude(Base.Vector(0,0,self.CaseBottomHeight))
        rod = Part.makeCylinder(self.__WallThick,self.CaseBottomHeight,
                                self.origin.add(Base.Vector(self.__WallThick,
                                                            self.CaseLength-self.__WallThick,0)))
        #self.t = temp
        #self.r = rod
        bot = bot.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              self.origin.add(Base.Vector(self.CaseWidth-self.__WallThick,0,0))
                              ).extrude(Base.Vector(0,0,self.CaseBottomHeight))
        rod = Part.makeCylinder(self.__WallThick,self.CaseBottomHeight,
                                self.origin.add(Base.Vector(self.CaseWidth-self.__WallThick,self.__WallThick,0)))
        #self.t = temp
        #self.r = rod
        bot = bot.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              self.origin.add(Base.Vector(self.CaseWidth-self.__WallThick,
                                                          self.CaseLength-self.__WallThick,0))
                              ).extrude(Base.Vector(0,0,self.CaseBottomHeight))
        rod = Part.makeCylinder(self.__WallThick,self.CaseBottomHeight,
                                self.origin.add(Base.Vector(self.CaseWidth-self.__WallThick,
                                                            self.CaseLength-self.__WallThick,0)))
        #self.t = temp
        #self.r = rod
        bot = bot.cut(temp.cut(rod))

        taborigin = self.origin.add(Base.Vector(-self.__MountTabLength,
                                                self.MountTabCY1-(self.__MountTabWidth/2),
                                                0))
        tab = Part.makePlane(self.__MountTabLength+self.__WallThick,
                             self.__MountTabWidth,taborigin)\
                .extrude(Base.Vector(0,0,self.__WallThick))
        temp = Part.makePlane(self.__MountTabWidth/2,
                              self.__MountTabWidth,
                              taborigin).extrude(Base.Vector(0,0,self.__WallThick))
        rod = Part.makeCylinder(self.__MountTabWidth/2,
                                self.__WallThick,
                                taborigin.add(Base.Vector(self.__MountTabWidth/2,
                                                          self.__MountTabWidth/2,0)))
        #self.t = temp
        #self.r = rod
        tab = tab.cut(temp.cut(rod))

        hole = Part.makeCylinder(self.__MountTabHoleDiameter/2,
                                self.__WallThick,
                                taborigin.add(Base.Vector(self.__MountTabWidth/2,
                                                          self.__MountTabWidth/2,0)))
        tab = tab.cut(hole)
        bot = bot.fuse(tab)
        taborigin = self.origin.add(Base.Vector(-self.__MountTabLength,
                                                self.MountTabCY2-(self.__MountTabWidth/2),
                                                0))
        tab = Part.makePlane(self.__MountTabLength+self.__WallThick,
                             self.__MountTabWidth,taborigin)\
                .extrude(Base.Vector(0,0,self.__WallThick))
        temp = Part.makePlane(self.__MountTabWidth/2,
                              self.__MountTabWidth,
                              taborigin).extrude(Base.Vector(0,0,self.__WallThick))
        rod = Part.makeCylinder(self.__MountTabWidth/2,
                                self.__WallThick,
                                taborigin.add(Base.Vector(self.__MountTabWidth/2,
                                                          self.__MountTabWidth/2,0)))
        #self.t = temp
        #self.r = rod
        tab = tab.cut(temp.cut(rod))
        hole = Part.makeCylinder(self.__MountTabHoleDiameter/2,
                                self.__WallThick,
                                taborigin.add(Base.Vector(self.__MountTabWidth/2,
                                                          self.__MountTabWidth/2,0)))
        tab = tab.cut(hole)
        bot = bot.fuse(tab)
        taborigin = self.origin.add(Base.Vector(self.CaseWidth-self.__WallThick,
                                                self.MountTabCY1-(self.__MountTabWidth/2),
                                                0))
        tab = Part.makePlane(self.__MountTabLength+self.__WallThick,
                             self.__MountTabWidth,taborigin)\
                .extrude(Base.Vector(0,0,self.__WallThick))
        temp = Part.makePlane(self.__MountTabWidth/2,
                              self.__MountTabWidth,
                              taborigin.add(Base.Vector(
                                self.__MountTabLength+self.__WallThick-self.__MountTabWidth/2,0,0))
                              ).extrude(Base.Vector(0,0,self.__WallThick))
        rod = Part.makeCylinder(self.__MountTabWidth/2,
                                self.__WallThick,
                                taborigin.add(Base.Vector(self.__MountTabLength+self.__WallThick-self.__MountTabWidth/2,
                                                          self.__MountTabWidth/2,0)))
        #self.t = temp
        #self.r = rod
        tab = tab.cut(temp.cut(rod))
        hole = Part.makeCylinder(self.__MountTabHoleDiameter/2,
                                self.__WallThick,
                                taborigin.add(Base.Vector(self.__MountTabLength+self.__WallThick-self.__MountTabWidth/2,
                                                          self.__MountTabWidth/2,0)))
        tab = tab.cut(hole)
        bot = bot.fuse(tab)
        taborigin = self.origin.add(Base.Vector(self.CaseWidth-self.__WallThick,
                                                self.MountTabCY2-(self.__MountTabWidth/2),
                                                0))
        tab = Part.makePlane(self.__MountTabLength+self.__WallThick,
                             self.__MountTabWidth,taborigin)\
                .extrude(Base.Vector(0,0,self.__WallThick))
        temp = Part.makePlane(self.__MountTabWidth/2,
                              self.__MountTabWidth,
                              taborigin.add(Base.Vector(
                                self.__MountTabLength+self.__WallThick-self.__MountTabWidth/2,0,0))
                              ).extrude(Base.Vector(0,0,self.__WallThick))
        rod = Part.makeCylinder(self.__MountTabWidth/2,
                                self.__WallThick,
                                taborigin.add(Base.Vector(self.__MountTabLength+self.__WallThick-self.__MountTabWidth/2,
                                                          self.__MountTabWidth/2,0)))
        #self.t = temp
        #self.r = rod
        tab = tab.cut(temp.cut(rod))
        hole = Part.makeCylinder(self.__MountTabHoleDiameter/2,
                                self.__WallThick,
                                taborigin.add(Base.Vector(self.__MountTabLength+self.__WallThick-self.__MountTabWidth/2,
                                                          self.__MountTabWidth/2,0)))
        #self.r = hole
        tab = tab.cut(hole)
        bot = bot.fuse(tab)
        boardPostOrigin = self.origin.add(Base.Vector(self.__WallThick,
                                                      self.__WallThick,
                                                      0))
        for post in Board.HoleOffsets():
            pX, pY = post
            postOrig = boardPostOrigin.add(Base.Vector(pX,pY,0))
            post = Part.makeCylinder(self.__PostDiameter/2,
                                     self.PostLength,
                                     postOrig)
            post = post.fuse(
                Part.makeCylinder(self.__PostShoulderDiameter/2,
                                  self.PostShoulderLength,
                                  postOrig)
            )
            bot = bot.fuse(post)
            bot = bot.cut(
                Part.makeCylinder(self.__PostShoulderInnerDiameter/2,
                                  self.__PostShoulderDepth,
                                  postOrig)
            )               
            bot = bot.cut(
                Part.makeCylinder(self.__PostHoleDiameter/2,
                                  self.PostLength,
                                  postOrig)
            )
        return bot
    def __LEDRod(self,posXY):
        ledX, ledY = posXY
        #debug("*** __LEDRod(): ledX = %f, ledY = %f",ledX,ledY)
        #debug("*** __LEDRod(): self.origin = (%f, %f, %f)",self.origin.x,
        #                                                   self.origin.y,
        #                                                   self.origin.z)
        ledOrig = self.origin.add(Base.Vector(self.__WallThick+ledX,
                                              self.__WallThick+ledY,
                                              self.CaseBottomHeight+self.__LEDZOffset))
        #debug("*** __LEDRod(): ledOrig = (%f, %f, %f)",ledOrig.x,ledOrig.y,ledOrig.z)
        rodLength = self.CaseTopHeight-self.__WallThick/2
        return Part.makeCylinder(self.__LEDRodDiameter/2,rodLength,ledOrig)
    def __top(self):
        toporigin = self.origin.add(Base.Vector(0,0,self.CaseBottomHeight))
        top = Part.makePlane(self.CaseWidth,self.CaseLength,
                             toporigin.add(Base.Vector(0,0,self.CaseTopHeight-\
                                                           self.__WallThick*1.5))) \
            .extrude(Base.Vector(0,0,self.__WallThick))
        top = top.fuse(
            Part.makePlane(self.CaseTopHeight,self.CaseLength,
                           toporigin.add(Base.Vector(0,self.CaseLength,
                                                     -self.__WallThick/2)),
                           Base.Vector(1,0,0)).extrude(Base.Vector(self.__WallThick,0,0))
        )
        top = top.cut(
            Part.makePlane(self.__WallThick/2,
                               self.CaseLength-self.__WallThick,
                               toporigin.add(Base.Vector(self.__WallThick/2,
                                             self.CaseLength-self.__WallThick/2,
                                             -self.__WallThick/2)),
                               Base.Vector(1,0,0)).extrude(Base.Vector(self.__WallThick/2,0,0))
        )
        top = top.fuse(
            Part.makePlane(self.CaseTopHeight,self.CaseLength,
                           toporigin.add(Base.Vector(self.CaseWidth-self.__WallThick,
                                                     self.CaseLength,
                                                     -self.__WallThick/2)),
                           Base.Vector(1,0,0)).extrude(Base.Vector(self.__WallThick,0,0))
        )
        top = top.cut(
            Part.makePlane(self.__WallThick/2,
                               self.CaseLength-self.__WallThick,
                               toporigin.add(Base.Vector(self.CaseWidth-self.__WallThick,
                                             self.CaseLength-self.__WallThick/2,
                                             -self.__WallThick/2)),
                               Base.Vector(1,0,0)).extrude(Base.Vector(self.__WallThick/2,0,0))
        )
        top = top.fuse(
            Part.makePlane(self.CaseTopHeight,self.CaseWidth,
                           toporigin.add(Base.Vector(0,0,-self.__WallThick/2)),
                           Base.Vector(0,1,0)).extrude(Base.Vector(0,self.__WallThick,0))
        )
        top = top.cut(
            Part.makePlane(self.__WallThick/2,self.CaseWidth-self.__WallThick,
                           toporigin.add(Base.Vector(self.__WallThick/2,
                                                     self.__WallThick/2,
                                                     -self.__WallThick/2)),
                           Base.Vector(0,1,0)).extrude(Base.Vector(0,self.__WallThick,0))
                           
        )
        top = top.fuse(
            Part.makePlane(self.CaseTopHeight,self.CaseWidth,
                           toporigin.add(Base.Vector(0,self.CaseLength-self.__WallThick,-self.__WallThick/2)),
                           Base.Vector(0,1,0)).extrude(Base.Vector(0,self.__WallThick,0))
        )
        top = top.cut(
            Part.makePlane(self.__WallThick/2,self.CaseWidth-self.__WallThick,
                           toporigin.add(Base.Vector(self.__WallThick/2,
                                                     self.CaseLength-self.__WallThick*1.5,
                                                     -self.__WallThick/2)),
                           Base.Vector(0,1,0)).extrude(Base.Vector(0,self.__WallThick,0))        
        )
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              toporigin.add(Base.Vector(0,0,self.CaseTopHeight-self.__WallThick*1.5)),
                              Base.Vector(0,1,0))\
                .extrude(Base.Vector(0,self.CaseLength,0))
        offset = Base.Vector(self.__WallThick,0,self.CaseTopHeight-self.__WallThick*1.5)
        rod = Part.makeCylinder(self.__WallThick,self.CaseLength,
                                toporigin.add(offset),Base.Vector(0,1,0))
        top = top.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              toporigin.add(Base.Vector(self.CaseWidth-self.__WallThick,
                                     0,self.CaseTopHeight-self.__WallThick*1.5)),
                              Base.Vector(0,1,0))\
                .extrude(Base.Vector(0,self.CaseLength,0))
        offset = Base.Vector(self.CaseWidth-self.__WallThick,0,self.CaseTopHeight-self.__WallThick*1.5)
        rod = Part.makeCylinder(self.__WallThick,self.CaseLength,
                                toporigin.add(offset),Base.Vector(0,1,0))
        top = top.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              toporigin.add(Base.Vector(0,self.__WallThick,
                                     self.CaseTopHeight-self.__WallThick*1.5)),
                              Base.Vector(1,0,0)).extrude(Base.Vector(self.CaseWidth,0,0))
        offset = Base.Vector(0,self.__WallThick,self.CaseTopHeight-self.__WallThick*1.5)
        rod = Part.makeCylinder(self.__WallThick,self.CaseWidth,
                                toporigin.add(offset),Base.Vector(1,0,0))
        top = top.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              toporigin.add(Base.Vector(0,self.CaseLength,
                                self.CaseTopHeight-self.__WallThick*1.5)),
                              Base.Vector(1,0,0)).extrude(Base.Vector(self.CaseWidth,0,0))
        offset = Base.Vector(0,self.CaseLength-self.__WallThick,
                             self.CaseTopHeight-self.__WallThick*1.5)
        rod = Part.makeCylinder(self.__WallThick,self.CaseWidth,
                                toporigin.add(offset),Base.Vector(1,0,0))
        top = top.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              toporigin.add(Base.Vector(0,0,-self.__WallThick/2)))\
                                .extrude(Base.Vector(0,0,self.CaseTopHeight))
        rod = Part.makeCylinder(self.__WallThick,self.CaseTopHeight,
                                toporigin.add(Base.Vector(self.__WallThick,
                                                          self.__WallThick,
                                                          -self.__WallThick/2)))
        top = top.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              toporigin.add(Base.Vector(0,self.CaseLength-self.__WallThick,-self.__WallThick/2))
                              ).extrude(Base.Vector(0,0,self.CaseTopHeight))
        rod = Part.makeCylinder(self.__WallThick,self.CaseTopHeight,
                                toporigin.add(Base.Vector(self.__WallThick,
                                                            self.CaseLength-self.__WallThick,-self.__WallThick/2)))
        self.t = temp
        self.r = rod
        top = top.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              toporigin.add(Base.Vector(self.CaseWidth-self.__WallThick,0,-self.__WallThick/2))
                              ).extrude(Base.Vector(0,0,self.CaseTopHeight))
        rod = Part.makeCylinder(self.__WallThick,self.CaseTopHeight,
                                toporigin.add(Base.Vector(self.CaseWidth-self.__WallThick,self.__WallThick,-self.__WallThick/2)))
        self.t = temp
        self.r = rod
        top = top.cut(temp.cut(rod))
        temp = Part.makePlane(self.__WallThick,self.__WallThick,
                              toporigin.add(Base.Vector(self.CaseWidth-self.__WallThick,
                                                          self.CaseLength-self.__WallThick,-self.__WallThick/2))
                              ).extrude(Base.Vector(0,0,self.CaseTopHeight))
        rod = Part.makeCylinder(self.__WallThick,self.CaseTopHeight,
                                toporigin.add(Base.Vector(self.CaseWidth-self.__WallThick,
                                                            self.CaseLength-self.__WallThick,-self.__WallThick/2)))
        top = top.cut(temp.cut(rod))

        
        bjopening = Part.makePlane(self.__BJHeight+self.__WallThick/2,
                                   self.__BJWidth,
                                   toporigin.add(Base.Vector(self.__BJXOffset+self.__WallThick,
                                                             self.CaseLength-self.__WallThick,
                                                             -self.__WallThick/2)),
                                   Base.Vector(0,1,0)).extrude(Base.Vector(0,self.__WallThick,0))
        #self.t = bjopening
        top = top.cut(bjopening)
        usbaopening = Part.makePlane(self.__USBAHeight+self.__WallThick/2,
                                     self.__USBAWidth,
                                     toporigin.add(Base.Vector(0,self.__USBAYOffset+self.__USBAWidth+self.__WallThick,-self.__WallThick/2)),
                                     Base.Vector(1,0,0))\
                 .extrude(Base.Vector(self.__WallThick,0,0))
        #self.t = usbaopening
        top = top.cut(usbaopening)
        lccpoutopening = Part.makePlane(self.__LCCPowerPlugHeight+self.__WallThick/2,
                                        self.__LCCPowerPlugWidth,
                                        toporigin.add(
                                            Base.Vector(self.__LCCPOutXOffset+self.__WallThick,
                                                        0,
                                                        -self.__WallThick/2)),
                                        Base.Vector(0,1,0))\
                        .extrude(Base.Vector(0,self.__WallThick,0))
        self.t = lccpoutopening
        top = top.cut(lccpoutopening)
        lccrj45sopening = Part.makePlane(self.__LCCRJ45Height+self.__WallThick/2,
                                         self.__LCCRJ45Width,
                                         toporigin.add(
                                            Base.Vector(self.__LCCRJ45XOffset+self.__WallThick,
                                                        0,
                                                        -self.__WallThick/2)),
                                         Base.Vector(0,1,0))\
                        .extrude(Base.Vector(0,self.__WallThick,0))
        self.t = lccrj45sopening
        top = top.cut(lccrj45sopening)
        lccpinopening = Part.makePlane(self.__LCCPowerPlugHeight+self.__WallThick/2,
                                        self.__LCCPowerPlugWidth,
                                        toporigin.add(
                                            Base.Vector(self.__LCCPInXOffset+self.__WallThick,
                                                        0,
                                                        -self.__WallThick/2)),
                                        Base.Vector(0,1,0))\
                        .extrude(Base.Vector(0,self.__WallThick,0))
        self.t = lccpinopening
        top = top.cut(lccpinopening)
        trackpoweropening = Part.makePlane(self.__TrackPowerHeight+self.__WallThick/2,
                                        self.__TrackPowerWidth,
                                        toporigin.add(
                                            Base.Vector(self.__TrackPowerXOffset+self.__WallThick,
                                                        0,
                                                        -self.__WallThick/2)),
                                        Base.Vector(0,1,0))\
                        .extrude(Base.Vector(0,self.__WallThick,0))
        self.t = trackpoweropening
        top = top.cut(trackpoweropening)
        self.lccTxAct = self.__LEDRod(self.__LCCTxAct)
        top = top.cut(self.lccTxAct)
        self.lccRxAct = self.__LEDRod(self.__LCCRxAct)
        top = top.cut(self.lccRxAct)
        self.opsBAct  = self.__LEDRod(self.__OPSBAct)
        top = top.cut(self.opsBAct)
        self.opsAAct  = self.__LEDRod(self.__OPSAAct)
        top = top.cut(self.opsAAct)
        self.progBAct  = self.__LEDRod(self.__ProgBAct)
        top = top.cut(self.progBAct)
        self.progAAct  = self.__LEDRod(self.__ProgAAct)
        top = top.cut(self.progAAct)
        self.power     = self.__LEDRod(self.__Power)
        top = top.cut(self.power)

        boardPostOrigin = toporigin.add(Base.Vector(self.__WallThick,         
                                                    self.__WallThick,
                                                    0))
        for post in Board.HoleOffsets():
            pX, pY = post
            postOrig = boardPostOrigin.add(Base.Vector(pX,pY,0))
            post = Part.makeCylinder(self.__PostDiameter/2,
                                     self.TopPostLength,
                                     postOrig)
            post = post.cut(
                Part.makeCylinder(self.__TopPostDrillDiameter/2,
                                  self.TopPostDrillDepth,
                                  postOrig)
            )
            top = top.fuse(post)
        for ventCol in range(0,10):
            ventY = self.__WallThick+5.2755+ventCol*10.5510
            for ventRow in range(0,2):
                ventZ = 8.4200+ventRow*8.4200
                hole = Part.makeCylinder(self.__VentHoleDiameter/2,
                                         self.__WallThick,
                                         toporigin.add(
                                            Base.Vector(
                                                self.CaseWidth-self.__WallThick,
                                                ventY,ventZ)),
                                         Base.Vector(1,0,0))
                top = top.cut(hole)
                if ventRow == 1 or \
                   ventY > (self.__USBAYOffset+self.__USBAWidth+self.__VentHoleDiameter) or \
                   ventY < (self.__USBAYOffset):
                    hole = Part.makeCylinder(self.__VentHoleDiameter/2,
                                             self.__WallThick,
                                             toporigin.add(
                                                Base.Vector(0,ventY,ventZ)),
                                             Base.Vector(1,0,0))
                    top = top.cut(hole)
        for ventCol in range(0,5):
            ventX = 2.5+(ventCol*5)
            for ventRow in range(0,5):
                if (ventCol == 0 or ventCol == 4) and \
                   (ventRow == 0 or ventRow == 4):
                    continue
                ventY = 2.5+(ventRow*5)
                hole = Part.makeCylinder(self.__VentHoleDiameter/2,
                                         self.__WallThick,
                                         toporigin.add(
                                            Base.Vector(
                                                self.__FANXOffset+self.__WallThick+ventX,
                                                self.__FANYOffset+self.__WallThick+ventY,
                                                self.CaseTopHeight-self.__WallThick*1.5)))
                top = top.cut(hole)
        return top
    def __init__(self,name,origin=Base.Vector(0,0,0),fakeboard=True):
        self.name = name
        if not isinstance(origin,Base.Vector):
            raise RuntimeError("origin is not a Vector!")
        self.origin = origin
        self.bottom = self.__bottom()
        if fakeboard:
            self.board = Board(name+"_board",
                               self.origin.add(Base.Vector(self.__WallThick,
                                                           self.__WallThick,
                                                           self.CaseBottomHeight-self.__BoardThick)))
        else:
            self.board = None
        self.top = self.__top()
        self.bottom = self.bottom.cut(self.top)
    def show(self,doc=None):
        if doc==None:
            doc = App.activeDocument()
        obj = doc.addObject("Part::Feature",self.name+"_bottom")
        obj.Shape = self.bottom
        obj.Label=self.name+"_bottom"
        obj.ViewObject.ShapeColor=(0.5,0.5,0.5)
        if self.board != None:
            self.board.show(doc)
        obj = doc.addObject("Part::Feature",self.name+"_top")
        obj.Shape = self.top
        obj.Label=self.name+"_top"
        obj.ViewObject.ShapeColor=(0.5,0.5,0.5)
        obj = doc.addObject("Part::Feature",self.name+"_lccTxAct")
        obj.Shape = self.lccTxAct
        obj.Label=self.name+"_lccTxAct"
        obj.ViewObject.ShapeColor=(1.0,1.0,1.0)
        obj.ViewObject.Transparency=90
        obj = doc.addObject("Part::Feature",self.name+"_lccRxAct")
        obj.Shape = self.lccRxAct
        obj.Label=self.name+"_lccRxAct"
        obj.ViewObject.ShapeColor=(1.0,1.0,1.0)
        obj.ViewObject.Transparency=90
        obj = doc.addObject("Part::Feature",self.name+"_opsBAct")
        obj.Shape = self.opsBAct
        obj.Label=self.name+"_opsBAct"
        obj.ViewObject.ShapeColor=(1.0,1.0,1.0)
        obj.ViewObject.Transparency=90
        obj = doc.addObject("Part::Feature",self.name+"_opsAAct")
        obj.Shape = self.opsAAct
        obj.Label=self.name+"_opsAAct"
        obj.ViewObject.ShapeColor=(1.0,1.0,1.0)
        obj.ViewObject.Transparency=90
        obj = doc.addObject("Part::Feature",self.name+"_progBAct")
        obj.Shape = self.progBAct
        obj.Label=self.name+"_progBAct"
        obj.ViewObject.ShapeColor=(1.0,1.0,1.0)
        obj.ViewObject.Transparency=90
        obj = doc.addObject("Part::Feature",self.name+"_progAAct")
        obj.Shape = self.progAAct
        obj.Label=self.name+"_progAAct"
        obj.ViewObject.ShapeColor=(1.0,1.0,1.0)
        obj.ViewObject.Transparency=90
        obj = doc.addObject("Part::Feature",self.name+"_power")
        obj.Shape = self.power
        obj.Label=self.name+"_power"
        obj.ViewObject.ShapeColor=(1.0,1.0,1.0)
        obj.ViewObject.Transparency=90
