    __LCCRxAct = (156.0322-85.09, 105.51-(162.0775-58.42))
    __OPSBAct  = (172.1104-85.09, 105.51-(162.0519-58.42))
    __OPSAAct  = (173.8376-85.09, 105.51-(162.0773-58.42))
